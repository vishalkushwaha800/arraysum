
public class arraysum {
    public static void main (String[]args){
        double[] value = {7.0, 7.1, 7.2, 7.3, 7.4};
        double sum = 0;

        for (int i = 0; i < value.length; i++) {
            System.out.println(value[i]);
        }

        for (int i = 0; i< value.length; i++) {
            sum += value[i];
        }


            System.out.println("The sum of the array :" + sum);



    }



}
